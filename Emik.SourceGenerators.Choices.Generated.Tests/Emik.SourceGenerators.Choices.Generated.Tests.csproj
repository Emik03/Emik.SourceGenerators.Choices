<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <IsKtane>true</IsKtane>
        <OutputType>Exe</OutputType>
        <TargetFramework>net9.0</TargetFramework>
        <DisableDefaultDocumentation>true</DisableDefaultDocumentation>
        <NoWarn>$(NoWarn);AsyncifyInvocation;MSB3277;VSTHRD105</NoWarn>
        <RepositoryUrl>https://github.com/Emik03/Emik.SourceGenerators.Choices</RepositoryUrl>
        <Description>Annotate [Choice] to transform your type into a highly performant and flexible disjoint union.</Description>
        <PackageTags>Analyzer Coproduct Delegate Discriminated Disjoint Either Forwarder Functional Generator Infer Inference Interface Monad Monadic Monoid Performant Roslyn Source Union</PackageTags>
    </PropertyGroup>
    <ItemGroup>
        <Compile Remove="$(Morsels)/Compile/**/*.cs"/>
    </ItemGroup>
</Project>
